{
  "name": "loader-url",
  "version": "0.7.0",
  "description": "CLI-app load html page on url into selected directory",
  "main": "dist/index.js",
  "bin": {
    "page-loader": "dist/bin/page-loader.js"
  },
  "engines": {
    "node": "9"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "DEBUG=page-loader jest",
    "watch": "DEBUG=page-loader jest --watch",
    "babel-node": "babel-node",
    "eslint": "eslint",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/__fixtures__/"
    ],
    "verbose": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yorickov/loader-url.git"
  },
  "bugs": {
    "url": "https://github.com/Yorickov/loader-url/issues"
  },
  "keywords": [],
  "author": "Yorickov",
  "license": "ISC",
  "homepage": "https://github.com/Yorickov/loader-url#readme",
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-jest": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-env": "*",
    "babel-preset-stage-0": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "jest": "*",
    "jest-cli": "*",
    "nock": "^9.2.3",
    "regenerator-runtime": "*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.14.1",
    "debug": "^3.1.0",
    "listr": "^0.13.0",
    "lodash": "^4.17.5",
    "mz": "^2.7.0"
  }
}
